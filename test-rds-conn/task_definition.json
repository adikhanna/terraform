[
    {
      "cpu": 256,
      "image": "",
      "memory": 512,
      "name": "app",
      "networkMode": "awsvpc",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "test-rds-log-group",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "test-rds-conn"
        }
    },
      "command": ["/bin/sh -c 'psql --dbname=$DB_NAME --host=$DB_HOST --username=$DB_USER --password=$DB_PASSWORD'"],
      "entryPoint": ["sh", "-c"],
      "environment": [
        {
            "name": "DB_HOST",
            "value": "${db_host}"
        },
        {
            "name": "DB_USER",
            "value": "${db_user}"
        },
        {
            "name": "DB_PASSWORD",
            "value": "${db_password}"
        },
        {
            "name": "DB_NAME",
            "value": "${db_name}"
        }
        ]
    }
  ]